<script type="text/javascript">
  $('a[data-toggle="collapse"]').click(function(event) {
    deleteColorRedAccordion();

    if ($(this).hasClass('collapsed')) {
      $(this).css('color', '#ff0b0f').closest('.card-header')
              .addClass('border-red').find('.icon-carta i').addClass('fa-minus');      
    }
  });

  function deleteColorRedAccordion(){
    $links = $('a[data-toggle="collapse"]');
    $links.each(function(index, el) {
      $(this).css('color', '').closest('.card-header')
              .removeClass('border-red').find('.icon-carta i').removeClass('fa-minus').addClass('fa-plus');
    });
  }
</script>

